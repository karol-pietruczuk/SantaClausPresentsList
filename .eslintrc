{
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": ["error",  { "allow": ["_checkRecord", "_validate"] }],
    "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["record"] }]
  }
}